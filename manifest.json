{
    "manifest_version": 3,
    "name": "Screenshot Extension",
    "version": "1.0",
    "description": "Takes a screenshot of the browser and saves it to the desktop.",
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  